{
  "cSpell.words": [
    "attributify",
    "esbuild",
    "sxzz",
    "tsup",
    "unocss",
    "unplugin",
    "vitest",
    "vmodel",
    "vueuse"
  ],
  "explorer.fileNesting.patterns": {
    "index.ts": "rollup.ts, vite.ts, webpack.ts, esbuild.ts, rspack.ts, rolldown.ts",
    "macros.d.ts": "macros*.d.ts"
  },
  "exportall.config.folderListener": [
    "/packages/api/src/vue",
    "/packages/api/src/ts",
    "/packages/config/src"
  ],
  "exportall.config.semis": false,
  "search.exclude": {
    "**/CHANGELOG.md": true,
    "**/coverage": true,
    "**/dist": true,
    "**/node_modules": true,
    "docs/.vitepress/cache": true
  },
  "unocss.root": ["./", "./playground/vue3", "./packages/devtools/src/client"]
}
