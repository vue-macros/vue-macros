{
  "name": "monorepo-unplugin-vue-macros",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.0.0",
  "description": "Monorepo for Vue Macros.",
  "type": "module",
  "author": "三咲智子 <sxzz@sxzz.moe>",
  "scripts": {
    "build": "pnpm run --filter=\"./packages/*\" --parallel build",
    "dev": "pnpm run --filter=\"./packages/*\" --parallel dev",
    "clean": "rimraf packages/*/dist",
    "docs:dev": "pnpm run -C ./docs dev",
    "docs:preview": "pnpm run -C ./docs preview",
    "docs:build": "pnpm run -C ./docs build",
    "play": "pnpm run play:vue3",
    "play:vue2": "pnpm run -C ./playground/vue2 dev",
    "play:vue3": "pnpm run -C ./playground/vue3 dev",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ecosystem": "run-p test typecheck",
    "lint": "eslint --max-warnings 0 --cache .",
    "cspell:check": "cspell \"**\" --no-progress",
    "format": "prettier --write --cache .",
    "format:check": "prettier --check --cache .",
    "mono": "monoman",
    "mono:check": "monoman --check",
    "publint": "tsx publint.ts",
    "publish": "pnpm build && changeset publish",
    "typecheck": "run-p -l \"typecheck:**\"",
    "typecheck:lib": "vue-tsc --noEmit -p tsconfig.lib.json",
    "typecheck:test": "vue-tsc --noEmit -p tsconfig.test.json",
    "typecheck:docs": "vue-tsc --noEmit -p ./docs/tsconfig.json",
    "typecheck:fixture": "vue-tsc --noEmit -p tsconfig.fixture.json",
    "typecheck:playground:vue3": "pnpm run -C ./playground/vue3 typecheck",
    "typecheck:playground:vue2": "pnpm run -C ./playground/vue2 typecheck"
  },
  "devDependencies": {
    "@babel/types": "catalog:",
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.11",
    "@rspack/core": "^1.1.8",
    "@sxzz/eslint-config": "^4.6.0",
    "@sxzz/prettier-config": "^2.0.2",
    "@types/node": "^22.10.7",
    "@vitest/coverage-v8": "catalog:",
    "@vitest/ui": "catalog:",
    "@vue-macros/test-utils": "workspace:*",
    "change-case": "^5.4.4",
    "cspell": "^8.17.2",
    "esbuild": "catalog:",
    "eslint": "^9.18.0",
    "expect-type": "^1.1.0",
    "fast-glob": "^3.3.3",
    "importx": "^0.5.1",
    "monoman": "^0.6.2",
    "npm-run-all2": "^7.0.2",
    "oxc-transform": "^0.47.0",
    "pkg-types": "^1.3.1",
    "prettier": "^3.4.2",
    "publint": "^0.3.2",
    "radix-vue": "^1.9.12",
    "rimraf": "^6.0.1",
    "rolldown": "^1.0.0-beta.1",
    "rollup": "catalog:",
    "rollup-plugin-dts": "^6.1.1",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "catalog:",
    "unocss": "catalog:",
    "unplugin": "catalog:",
    "unplugin-isolated-decl": "^0.10.5",
    "unplugin-macros": "^0.14.0",
    "unplugin-raw": "^0.3.1",
    "unplugin-unused": "^0.3.0",
    "unplugin-vue-macros": "workspace:*",
    "vite": "catalog:",
    "vitest": "catalog:",
    "vue": "catalog:",
    "vue-tsc": "catalog:",
    "vue2": "npm:vue@^2.7.16",
    "webpack": "^5.97.1"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "nuxt",
        "@algolia/client-search",
        "search-insights",
        "pug"
      ],
      "allowAny": [
        "vue"
      ]
    },
    "overrides": {
      "@vue/language-core": "catalog:",
      "esbuild": "catalog:",
      "is-core-module": "npm:@no-shims/is-core-module",
      "smartwrap": "npm:@no-shims/smartwrap",
      "vite": "catalog:"
    },
    "patchedDependencies": {
      "@nolebase/ui": "patches/@nolebase__ui.patch"
    }
  },
  "prettier": "@sxzz/prettier-config"
}
